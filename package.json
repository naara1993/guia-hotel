  "scripts": {
    "dev": "lite-server",
    "start": "concurrently \"npm run watch:scss\" \"npm run dev\"  ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/",
    "clean": "rimraf dist",
    "compress:images": "imagemin src/images/* -o dist/images",
    "quux": "imagemin images/* --out-dir=dist",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin about.html -d dist --htmlmin -o dist/about.html && usemin precio.html -d dist --htmlmin -o dist/precio.html  && usemin contactos.html -d dist --htmlmin -o dist/contactos.html ",
    "build": "npm run clean  && npm run scss && npm run imagemin && npm run usemin",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "watch:html": "onchange src/index.html src/pages -- npm-run-all clean:html build:html",
    "watch:css": "onchange src/scss -- npm-run-all clean:css build:css",
    "watch:js": "onchange src/js -- npm-run-all clean:js build:js",
    "watch:images": "onchange src/images -- npm-run-all clean:images build:images",
    "run:server": "browser-sync dist -w",
    "watch:all": "parallelshell \"npm run watch:scss\" \"npm run lite\""
  },
